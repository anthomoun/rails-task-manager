<div class="mb-3 text-decoration-none">
<h1 class="display-1">task list</h1>
<ul>
  <% @tasks.each do |task| %>
  <div>
    <li class="form-check">
      <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
      <label class="form-check-label" for="flexCheckDefault">
      </label><%= link_to task.title, task_path(task), class: "text-decoration-none" %></li>
  </div>
  <% end %>
</ul>
</div>
